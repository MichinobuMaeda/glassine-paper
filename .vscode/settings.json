{
  "cSpell.words": [
    "apdisk",
    "argb",
    "Cantarell",
    "checkmark",
    "cleancss",
    "dbnavigator",
    "desaturated",
    "donotpresent",
    "dotenv",
    "ehthumbs",
    "encryptable",
    "esktop",
    "eslintcache",
    "fontawesome",
    "fseventsd",
    "icns",
    "jscoverage",
    "jspm",
    "jsxs",
    "Maeda",
    "Michinobu",
    "mpeltonen",
    "msix",
    "netrwhist",
    "Neue",
    "Nuxt",
    "nvmrc",
    "pids",
    "pkgs",
    "precaching",
    "replstate",
    "RRGGBB",
    "Segoe",
    "Sessionx",
    "smakss",
    "sonarlint",
    "stylelintcache",
    "Sveltekit",
    "textareas",
    "timemachine",
    "Vite",
    "vitepress",
    "vuepress",
    "wscript",
    "Xuan"
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.validate.enable": false,
  "typescript.validate.enable": true,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "jsx",
    "typescript",
    "typescriptreact"
  ],
  "eslint.useFlatConfig": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.debug": true,
  "editor.linkedEditing": true,
  "livePreview.defaultPreviewPath": "/docs/sample",
  "livePreview.serverRoot": "${workspaceFolder}/docs",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.suggest.autoImports": true,
  "typescript.preferences.quoteStyle": "single",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
